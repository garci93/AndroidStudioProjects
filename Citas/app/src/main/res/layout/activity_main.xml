<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btnBorrar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Borrar"
        tools:layout_editor_absoluteX="171dp"
        tools:layout_editor_absoluteY="134dp" />

    <Button
        android:id="@+id/btnNueva"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/InsertarCita"
        tools:layout_editor_absoluteX="163dp"
        tools:layout_editor_absoluteY="225dp" />

    <Button
        android:id="@+id/btnVerTodas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ListarCitas"
        tools:layout_editor_absoluteX="175dp"
        tools:layout_editor_absoluteY="329dp" />

    <Button
        android:id="@+id/btnCitaDia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/CitaDia"
        tools:layout_editor_absoluteX="174dp"
        tools:layout_editor_absoluteY="442dp" />

    <Button
        android:id="@+id/btnConfigurar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Configuracion"
        tools:layout_editor_absoluteX="184dp"
        tools:layout_editor_absoluteY="537dp" />

</androidx.constraintlayout.widget.ConstraintLayout>